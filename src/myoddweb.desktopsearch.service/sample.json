{
  "MaxNumCharactersPerWords": 255,
  "MaxNumCharactersPerParts": 25,

  "WebServer": {
    "Port": 9501
  },
  "Loggers": [
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Type": "Console"
    },
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Type": "File",
      "Path": "./"
    }
  ],
  "Timers": {
    "EventsParserMs": 30000
  },
  "Processors": {
    "ConcurrentDirectoriesProcessor": 2,
    "ConcurrentFilesProcessor": 8,
    "UpdatesPerFilesEvent": 100,
    "UpdateFileIdsEvent": 2,
    "QuietEventsProcessorMs": 10000,
    "BusyEventsProcessorMs": 1,
    "IgnoreFiles": [
      {
        "Pattern": "*.*",
        "MaxSizeMegabytes": "1024"
      }
    ]
  },
  "Paths": {
    "ParseFixedDrives": true,
    "ParseRemovableDrives": false,
    "Paths": [
      "c:\\Folder",
      "c:\\other"
    ],
    "IgnoredPaths": [
      "%temp%",
      "%tmp%",
      "%ProgramFiles%",
      "%ProgramFiles(x86)%",
      "%ProgramW6432%",
      "%windir%",
      "%SystemRoot%"
    ],
    "ComponentsPaths": [
      ".\\"
    ],
    "IgnoreInternetCache": true,
    "IgnoreRecycleBins": true,
    "IgnoreCurrentPath": true
  },
  "Database": {
    "Source": ".\\database.db",
    "IgnoredPaths": [ ".\\" ],
    "CacheSize": -8000
  },
  "Performance": {
    "CategoryName": "myoddweb.desktopsearch",
    "CategoryHelp": "Performance counters for myoddweb.desktopsearch",
    "DeleteStartUp": true
  } 
}
