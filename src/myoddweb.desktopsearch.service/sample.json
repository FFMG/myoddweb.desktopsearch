{
  "MaxNumCharacters" : 25,

  "WebServer": {
    "Port": 9501
  },
  "Loggers": [
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Type": "Console"
    },
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Type": "File",
      "Path": "./"
    }
  ],
  "Timers": {
    "EventsParserMs": 30000
  },
  "Processors": {
    "ConcurrentDirectoriesProcessor": 2,
    "ConcurrentFilesProcessor": 8,
    "UpdatesPerFilesEvent": 100,
    "QuietEventsProcessorMs": 10000,
    "BusyEventsProcessorMs" :  1 
  },
  "Paths": {
    "ParseFixedDrives": true,
    "ParseRemovableDrives": false,
    "Paths": [
      "c:\\Folder",
      "c:\\other"
    ],
    "IgnoredPaths": [
      "%temp%",
      "%tmp%",
      "%ProgramFiles%",
      "%ProgramFiles(x86)%",
      "%ProgramW6432%",
      "%windir%",
      "%SystemRoot%"
    ],
    "ComponentsPaths": [
      ".\\"
    ],
    "IgnoreInternetCache": true,
    "IgnoreRecycleBins": true
  }
}
