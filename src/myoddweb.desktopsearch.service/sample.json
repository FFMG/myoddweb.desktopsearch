{
  "Loggers": [
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Error", "Warning", "Information"
    }
  ],
  "Timers": {
    "EventsParserMs": 10000
  },
  "Paths": {
    "ParseFixedDrives": true,
    "ParseRemovableDrives": false,
    "Paths": [
      "c:\\Folder",
      "c:\\other"
    ],
    "IgnoredPaths": [
      "%temp%",
      "%tmp%",
      "%ProgramFiles%",
      "%ProgramFiles(x86)%",
      "%ProgramW6432%",
      "%windir%"
    ]
  }
}
