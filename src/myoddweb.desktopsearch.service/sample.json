{
  "WebServer": {
    "Port": 9501
  },
  "Loggers": [
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Type": "Console"
    },
    {
      "LogLevels": [
        "Error",
        "Warning",
        "Information"
      ],
      "Type": "File",
      "Path": "./"
    }
  ],
  "Timers": {
    "EventsParserMs": 30000
  },
  "Processors": {
    "ConcurrentDirectoriesProcessor": 15,
    "ConcurrentFilesProcessor": 25
  },
  "Paths": {
    "ParseFixedDrives": true,
    "ParseRemovableDrives": false,
    "Paths": [
      "c:\\Folder",
      "c:\\other"
    ],
    "IgnoredPaths": [
      "%temp%",
      "%tmp%",
      "%ProgramFiles%",
      "%ProgramFiles(x86)%",
      "%ProgramW6432%",
      "%windir%"
    ],
    "ComponentsPaths": [
      ".\\"
    ]
  }
}
